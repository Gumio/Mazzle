      margin: 40px;
      width: 250px;
      height: 400px;
      text-align: center;
      border-radius:30px;
      -webkit-border-radius:30px;
      -moz-border-radius:30px;
      float: left;
    }

    .productImg{
      width: 100%;
      height: 260px;
      background-size: auto;
      background-repeat: no-repeat;
      background-image: url("/product.jpeg");
      vertical-align: bottom;
      position: relative;

      -webkit-transition-duration: 0.5s;
      -webkit-transition-timing-function: linear;
      filter: brightness(70%);
      color: #ffffff;

    }
    .productImg:hover{
      filter: brightness(100%);
    }

    .productTitle{
      position: absolute;
      bottom: 50px;
    }

    .productDetail{
      position: absolute;
      bottom: 0px;
    }

    .favo{
      margin: 6px auto;
      width: 100px;
      padding: 5px;
      border: 1px solid #808686;
    }

    #contentTitle{
